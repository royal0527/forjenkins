pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
         stage('Build') {
            steps {
                echo 'In Building Stage'
            }
        }
         stage('deploy') {
            steps {
                echo 'deploy stage'
            }
        }
         stage('Test') {
            steps {
                echo 'Testing Stage'
            }
        }
         stage('Releasing') {
            steps {
                echo 'Releasing Stage'
            }
        }
    }
}
